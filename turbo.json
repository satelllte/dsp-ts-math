{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "generate:docs": {
      "outputs": ["docs/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "test:types": {},
    "test:lint": {
      "dependsOn": ["^build"]
    }
  }
}
