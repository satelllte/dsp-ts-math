{
  "name": "@dsp-ts/math",
  "version": "0.1.0",
  "license": "MIT",
  "author": "@satelllte (https://github.com/satelllte)",
  "description": "A tiny set of TypeScript math helpers related to sound processing (DSP)",
  "keywords": [
    "dsp",
    "dsp-ts",
    "dsp-math"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "generate:docs": "typedoc",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "test:lint": "eslint ./src/**/* --max-warnings=0 --format=compact"
  },
  "homepage": "https://github.com/satelllte/dsp-ts-math#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satelllte/dsp-ts-math.git",
    "directory": "packages/math"
  },
  "bugs": {
    "url": "https://github.com/satelllte/dsp-ts-math/issues"
  }
}
